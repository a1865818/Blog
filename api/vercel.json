{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/posts/(.*)",
        "dest": "/api/routes/posts.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "/api/routes/auth.js"
      },
      {
        "src": "/upload",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
}