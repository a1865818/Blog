{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth/(.*)",
        "dest": "/api/routes/auth.js"
      },
      {
        "src": "/api/posts/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/users/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/upload",
        "dest": "/api/index.js"
      },
      {
        "src": "/",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ]
}
