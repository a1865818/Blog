{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/api/posts/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/api/users/(.*)",
        "dest": "/index.js"
      },
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        }
    ]
  }
  